{
  "name": "@js-bits/model",
  "version": "0.0.2",
  "description": "Abstract multi-purpose data model",
  "keywords": [
    "javascript",
    "model",
    "abstract",
    "data",
    "json",
    "object",
    "validation",
    "mvc",
    "application",
    "architecture"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./index.js"
    }
  },
  "scripts": {
    "build": "rollup ./index.js --format cjs --file dist/index.cjs --exports named",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose",
    "test-coverage": "yarn test --coverage",
    "test-watch": "yarn test --watch",
    "lint": "eslint '**/*.{js,jsx}' --ignore-pattern jsdoc",
    "jsdoc": "jsdoc ./src -d jsdoc",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/js-bits/model"
  },
  "author": "https://github.com/apolo-gh",
  "license": "ISC",
  "prettier": "@js-bits/formalinter/.prettierrc.json",
  "eslintConfig": {
    "extends": "@js-bits/eslint-config/.eslintrc.json"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@js-bits/enumerate": "^0.9.1"
  },
  "devDependencies": {
    "@js-bits/formalinter": "^0.3.1",
    "@types/jest": "^29.4.0",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "jsdoc": "^3.6.10",
    "rollup": "^3.15.0"
  },
  "jest": {
    "projects": [
      {
        "displayName": "node",
        "testEnvironment": "node"
      },
      {
        "displayName": "jsdom",
        "testEnvironment": "jsdom"
      }
    ]
  }
}
